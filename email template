<!-- ------------------------------------------- !KÓD PRO E-MAIL! NEMĚNIT------------------------------------------- -->
{%- macro format_price(price, thousand_separator=dict['thousand_separator'], decimal_separator=dict['decimal_separator'], decimal_places=dict['decimal_places']) -%}
{%- set price_as_string = price | string -%}
{%- set price_split = price_as_string.split('.') -%}
{%- set price_integer = price_split[0] -%}
{%- if price_split | count > 1 -%}
{%- set price_fraction = price_split[1] -%}
{%- if price_fraction | length < decimal_places -%}
{%- set price_fraction = price_fraction.ljust(decimal_places, '0') -%}
{%- else -%}
{%- set price_fraction = price_fraction[:decimal_places] -%}
{%- endif -%}
{%- else -%}
{%- set price_fraction = '' -%}
{%- endif -%}
{%- set formatted_price_integer = price_integer | reverse | batch(3) | map('join', '') | join(thousand_separator) | reverse -%}
{%- if price_fraction != '' -%}
{%- set formatted_price = formatted_price_integer ~ decimal_separator ~ price_fraction -%}
{%- else -%}
{%- set formatted_price = formatted_price_integer-%}
{%- endif -%}
{{- formatted_price -}} {%- endmacro -%}

{# konec snippetu #}

{%- macro formatted_price(price, decimal_places=dict['decimal_places']) -%}
{%- if decimal_places > 0 -%}
{%- set final_price = price | replace(",", ".") | float  -%}
{%- else -%}
{%- set final_price = price | replace(",", ".") | float | round | int  -%}
{%- endif -%}
{{- final_price -}}
{%- endmacro -%}


<!-- -------------------------------------------------------------------------------------------------------------------------------- -->
{# Kontrola počtu slov a přidání mezery na poslední jednu-dvě pozice #}
{% macro last_space_medium(product_name) %}
{{ product_name | truncate(35) | reverse | replace(" ", ";psbn&", 1) | reverse if (product_name | wordcount > 3 and product_name | wordcount < 5 ) else product_name | truncate(35) | reverse | replace(" ", ";psbn&", 2) | reverse if (product_name | wordcount > 4 ) else product_name | truncate(35) }}
{% endmacro %}
{% macro last_space_long(product_name) %}
{{ product_name | truncate(40) | reverse | replace(" ", ";psbn&", 1) | reverse if (product_name | wordcount > 3 and product_name | wordcount < 5 ) else product_name | truncate(40) | reverse | replace(" ", ";psbn&", 2) | reverse if (product_name | wordcount > 4 ) else product_name | truncate(40) }}
{% endmacro %}
{% macro last_space_short(product_name) %}
{{ product_name | truncate(30) | reverse | replace(" ", ";psbn&", 1) | reverse if (product_name | wordcount > 3 and product_name | wordcount < 5 ) else product_name | truncate(30) | reverse | replace(" ", ";psbn&", 1) | reverse if (product_name | wordcount > 4 ) else product_name | truncate(30) }}
{% endmacro %}


